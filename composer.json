        { "type": "vcs", "url": "git@github.com:Bitendian/tbp-jwt.git" },
        { "type": "vcs", "url": "git@github.com:Bitendian/tbp-i18n-manager.git" },
        { "type": "vcs", "url": "git@github.com:Bitendian/tbp-deploy.git" },
